{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  data() {\n    return {\n      tableData: [],\n      // 所有的数据\n      pageNum: 1,\n      // 当前的页码\n      pageSize: 10,\n      // 每页显示的个数\n      total: 0,\n      name: null,\n      fromVisible: false,\n      form: {},\n      user: JSON.parse(localStorage.getItem('xm-user') || '{}'),\n      rules: {\n        type: [{\n          required: true,\n          message: '请输入课程名称',\n          trigger: 'blur'\n        }]\n      },\n      ids: [],\n      teacherData: []\n    };\n  },\n  created() {\n    this.load(1);\n    this.loadteacher();\n  },\n  methods: {\n    choiceCourse(row) {\n      let data = {\n        studentId: this.user.id,\n        teacherId: row.teacherId,\n        courseId: row.id\n      };\n      this.$http.post('/edu/course/choice', data).then(result => {\n        if (result.data.code === 200) {\n          this.$message({\n            type: 'success',\n            message: '选课成功'\n          });\n        }\n      });\n    },\n    handleEdit(row) {\n      this.fromVisible = true;\n      this.form = row;\n    },\n    loadteacher() {\n      this.$http.post('/edu/teacher/list').then(result => {\n        if (result.data.code === 200) {\n          this.teacherData = result.data.data;\n        }\n      });\n    },\n    load(pageNum) {\n      this.pageNum = pageNum;\n      let data = {\n        pageNum: this.pageNum,\n        pageSize: this.pageSize,\n        name: this.name\n      };\n      this.$http.post('/edu/course/list', data).then(result => {\n        this.total = result.data.data.total;\n        this.tableData = result.data.data.records;\n      });\n    },\n    handleCurrentChange(val) {\n      this.load(val);\n    },\n    handleSelectionChange(val) {\n      this.ids = [];\n      val.forEach(item => this.ids.push(item.id));\n    },\n    handleAdd() {\n      this.form = {};\n      this.fromVisible = true;\n    },\n    delBatch() {\n      if (this.ids.length === 0) {\n        this.$message({\n          type: 'warning',\n          message: '请选中课程'\n        });\n        return;\n      }\n      this.$http.post('/edu/course/deleteBatch', this.ids).then(result => {\n        if (result.data.code === 200) {\n          this.$message({\n            type: 'success',\n            message: '批量删除成功'\n          });\n          this.load(this.pageNum);\n        }\n      });\n    },\n    del(id) {\n      this.$confirm('是否要删除这条数据?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        this.$http.post('/edu/course/delete', id).then(result => {\n          if (result.data.code === 200) {\n            this.$message({\n              type: 'success',\n              message: '删除成功'\n            });\n            this.load(this.pageNum);\n          }\n        });\n      }).catch(() => {\n        this.$message({\n          type: 'info',\n          message: '取消删除'\n        });\n      });\n    },\n    reset() {\n      this.name = null;\n      this.load(1);\n    },\n    save() {\n      this.$refs['formRef'].validate(valid => {\n        if (valid) {\n          this.$http.post('/edu/course/saveOrUpdate', this.form).then(result => {\n            if (result.data.code === 200) {\n              this.$message({\n                type: 'success',\n                message: '操作成功'\n              });\n              this.fromVisible = false;\n              this.load(this.pageNum);\n            }\n          });\n        } else {\n          this.$message.error('请填写完整表单');\n          return false;\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["data","tableData","pageNum","pageSize","total","name","fromVisible","form","user","JSON","parse","localStorage","getItem","rules","type","required","message","trigger","ids","teacherData","created","load","loadteacher","methods","choiceCourse","row","studentId","id","teacherId","courseId","$http","post","then","result","code","$message","handleEdit